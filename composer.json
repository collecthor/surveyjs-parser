{
    "name": "collecthor/surveyjs-parser",
    "description": "Parse SurveyJS survey structure into PHP classes",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Collecthor\\SurveyjsParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Collecthor\\SurveyjsParser\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sam Mousa",
            "email": "sam@mousa.nl"
        }
    ],
    "require-dev": {
        "symplify/easy-coding-standard": ">= 11",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1",
        "ramsey/conventional-commits": "^1",
        "captainhook/plugin-composer": "^5.3",
        "nikic/iter": "^2",
        "phpstan/phpstan-phpunit": "^1",
        "phpstan/extension-installer": "^1",
        "phpstan/phpstan-strict-rules": "^1",
        "symfony/console": "< 6",
        "vimeo/psalm": "^4.26"
    },
    "require": {
        "php": ">= 8.1",
        "collecthor/data-interfaces": ">= 5.2"
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "test": "XDEBUG_MODE=coverage ./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan"
    }
}
